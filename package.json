{
	"name": "wdsit-com",
	"description": "Full-stack React Router 7 SSR application for Shopify plugin business.",
	"private": true,
	"dependencies": {
		"@react-router/express": "7.6.3",
		"express": "^4.21.2",
		"isbot": "5.1.26",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "7.6.3",
		"resend": "^4.0.1"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.10.2",
